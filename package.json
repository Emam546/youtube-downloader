{
  "name": "youtube-downloader",
  "version": "1.1.2",
  "main": "./out/main/index.js",
  "description": "A Youtube videos downloader app",
  "repository": "https://github.com/Emam546/youtube-downloader",
  "author": "Imam Ashour",
  "scripts": {
    "build:server": "npx ts-node build.ts",
    "build:next": "next build",
    "export": "npm run build:next&&next export",
    "build": "npm run build:server && npm run build:next",
    "lint": "next lint",
    "lint:server": "npx eslint -c server/.eslintrc.json --ext .ts server/src/",
    "start": "node -r module-alias/register ./dist --env=production",
    "dev": "nodemon",
    "test": "jest",
    "start:desk": "electron-vite preview",
    "build:electron": "electron-vite build",
    "dev-electron": "electron-vite dev",
    "dev:desk": "concurrently -n \"NEXT,ELECTRON\" -c \"yellow,blue\" --kill-others \"next dev\" \"npm run dev-electron\"",
    "build:desk": "npm run build:next&&next export -o ./out/renderer && npm run build:electron",
    "build:win": "npm run build:desk && electron-builder --win",
    "publish": "electron-builder -p always"
  },
  "nodemonConfig": {
    "watch": [
      "./server/src"
    ],
    "ext": "ts, html",
    "exec": "./node_modules/.bin/ts-node --project tsconfig.server.json --files -r tsconfig-paths/register ./server/src"
  },
  "_moduleAliases": {
    "@serv": "dist/"
  },
  "dependencies": {
    "@distube/ytdl-core": "^4.14.4",
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
