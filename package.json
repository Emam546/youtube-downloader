    "ytdl-core": "^4.11.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/bootstrap": "^5.2.6",
    "@types/command-line-args": "^5.2.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.16",
    "@types/find": "^0.2.1",
    "@types/fluent-ffmpeg": "^2.1.24",
