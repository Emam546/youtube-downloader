{
  "name": "youtube-playlist-server",
  "version": "0.0.0",
  "scripts": {
    "build:server": "npx ts-node build.ts",
    "build:next": "next build",
    "build": "npm run build:server && npm run build:next",
    "lint": "next lint",
    "lint:server": "npx eslint -c server/.eslintrc.json --ext .ts server/src/",
    "start": "node -r module-alias/register ./dist --env=production",
    "dev": "nodemon",
    "test": "jest"
  },
  "nodemonConfig": {
    "watch": [
      "./server/src"
    ],
    "ext": "ts, html",
    "exec": "./node_modules/.bin/ts-node --project tsconfig.server.json --files -r tsconfig-paths/register ./server/src"
  },
  "_moduleAliases": {
    "@serv": "dist/"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "@tanstack/react-query": "^4.26.1",
    "@tanstack/react-query-devtools": "^4.26.1",
    "axios": "^1.2.4",
    "bcrypt": "^5.1.0",
    "classnames": "^2.3.2",
    "command-line-args": "^5.2.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^6.0.1",
    "jet-logger": "^1.2.6",
    "jet-validator": "^1.0.4",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "next": "13.3.0",
    "node-cache": "^5.1.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^8.0.7",
    "youtube-playlists-js": "^1.0.3",
    "youtube-searches": "^1.0.1",
    "ytdl-core": "^4.11.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/bootstrap": "^5.2.6",
    "@types/command-line-args": "^5.2.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.16",
    "@types/find": "^0.2.1",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.4.0",
    "@types/jsonfile": "^6.1.1",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "autoprefixer": "^10.4.13",
    "bootstrap": "^5.3.0",
    "eslint": "^8.32.0",
    "eslint-config-next": "13.4.4",
    "eslint-plugin-node": "^11.1.0",
    "find": "^0.3.0",
    "fs-extra": "^11.1.0",
    "gh-pages": "^5.0.0",
    "jest": "^29.4.0",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.21",
    "sass": "^1.58.3",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.2.7",
    "ts-jest": "^29.0.5",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  }
}
